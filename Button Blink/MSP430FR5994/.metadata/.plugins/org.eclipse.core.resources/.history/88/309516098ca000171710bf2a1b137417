#include <msp430.h>

void main(void)
{
    PM5CTL0 &= ~LOCKLPM5;
    WDTCTL = WDTPW | WDTHOLD;       // stop watchdog timer
    P1DIR |= BIT1;                  // configure P1.6 as output


   int i = 0;   // volatile to prevent optimization


    while(1)
    {
        i ++;
        if(i>30000)
        {i=0;P1OUT^=BIT1;}

        }
    }



