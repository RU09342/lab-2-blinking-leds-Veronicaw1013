#include <msp430.h>



void main(void) {

    WDTCTL = WDTPW | WDTHOLD;       // Stop watchdog timer

    P1DIR |= BIT0;                  // Set P1.0 to output direction
    P1DIR |= BIT6;



    while(1) {

        volatile unsigned int i;    // volatile to prevent optimization



        P1OUT ^= BIT0;              // Toggle P1.0 using exclusive-OR



        i = 100000;                  // SW Delay

        do i--;

        while(i != 0);

        volatile unsigned int j;
        P1OUT ^= BIT6;
        j = 10000;
        do j--;
        while(i !=0);

    }



    //return ;

}
